{
  "require": {
    "barrelstrength/sprout-fields": "3.2.1",
    "charliedev/element-map": "1.2.1",
    "craftcms/ckeditor": "1.0.0-beta.2",
    "craftcms/cms": "^3.0.0",
    "craftcms/element-api": "2.5.4",
    "craftcms/guest-entries": "2.2.2.1",
    "elivz/craft-single-cat": "1.1.0",
    "markhuot/craftql": "1.3.1",
    "mildlygeeky/craft-kint": "1.0.0",
    "mmikkel/cp-field-inspect": "1.0.5",
    "nystudio107/craft-routemap": "1.1.5",
    "studioespresso/craft-dumper": "1.3.1",
    "venveo/craft-bulkedit": "1.0.3",
    "verbb/feed-me": "3.1.4",
    "verbb/super-table": "2.1.6",
    "vlucas/phpdotenv": "^2.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
